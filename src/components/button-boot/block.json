{
	"apiVersion": 2,
	"name": "dpwpblocks/button-boot",
	"title": "DP Button",
	"category": "design",
	"description": "Bootstrap & Custom Style Buttons also modal",
	"textdomain": "default",
	"attributes": {
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "href"
		},
		"buttonText" : {
			"type": "string",
			"source": "text",
			"selector": "a"
		},
		"color": {
			"type": "string",
			"default": ""
		},
		"bootAlign": {
			"type": "string",
			"default": ""
		},
		"bootClass": {
			"type": "string",
			"default": ""
		},
		"bootOutlineClass": {
			"type": "string",
			"default": ""
		},
		"bootSizesClass": {
			"type": "string",
			"default": ""
		},
		"bootPopClass": {
			"type": "string",
			"default": ""
		},
		"linkTarget": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "target"
		},
		"rel": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "rel"
		},
		"placeholder": {
			"type": "string"
		}
	},
	"supports": {
		"align": true
	}
}